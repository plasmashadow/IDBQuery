var db=new DataBase("testbase");
var dump={
	name:"person",
	properties:[
            {name:"person_name",type:"string"}
        ],
        foreignKeys:[
          {name:"user"}
        ]
};
var newdump={
   name:"user",
   properties:[
     {name:"user_id",type:"string",key:true},
     {name:"username",type:"string"}
   ]
};
db.addTable(newdump);
db.addTable(dump);
db.Initialize();
db.OnSave=function(event)
{
console.log(event.eventorigin);
}
db.Save("person",{person_id:1,person_name:"hello"});


var db=new DataBase("test");
db.addTable({
   name:"user",
   properties:[
     {name:"user_id",type:"number",key:true},
     {name:"username",type:"string"}
   ]
});
db.Initialize();



